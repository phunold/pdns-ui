%h2 Listing(brief, latest first)
%table
  %thead
    %tr
      %td{:id=>"cornerLeft",:width=>20} &nbsp;
      %td First Seen
      %td Last Seen
      %td DNS Query
      %td MAPTYPE
      %td DNS Answer
      %td{:id=>"cornerRight",:width=>20} &nbsp;
  - for p in @items
    %tbody
      %tr{:class=>cycle}
        %td &nbsp;
        %td= p[:FIRST_SEEN].strftime("%b %d, %T")
        - if p[:FIRST_SEEN] == p[:LAST_SEEN]
          %td same
        - else
          %td= p[:LAST_SEEN].strftime("%b %d, %T")
        %td
          %a{:href=>"/q/#{p[:QUERY]}"}= p[:QUERY]
        %td= p[:MAPTYPE]
        %td= p[:ANSWER]
        %td &nbsp;
